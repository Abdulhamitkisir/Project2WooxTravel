@model Project2WooxTravel.Entities.Admin
@{
    Layout = null;
}
<link href="~/Content/bootstrap.min.css" rel="stylesheet" />
<div class="container">
    <br /><br />
    <div class="alert alert-primary"><h3>Yeni Kullanici Kayit Sayfasi</h3></div>
    <label>Adiniz</label>
    @Html.TextBoxFor(x => x.Name, new { @class = "form-control" })
    <br />
    <label>Soyadiniz</label>
    @Html.TextBoxFor(x => x.Surname, new { @class = "form-control" })
    <br />
    <label>E-Mail Adresiniz</label>
    @Html.TextBoxFor(x => x.Email, new { @class = "form-control" })
    <br />
    <label>Kullanici Adiniz</label>
    @Html.TextBoxFor(x => x.Username, new { @class = "form-control" })
    <br />
    <label>Sifreniz</label>
    @Html.PasswordFor(x => x.Pasword, new { @class = "form-control" })
</div>
@model Project2WooxTravel.Entities.Admin
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    @Html.Action("PartialHead", "Default")
</head>
<body>

    @Html.Action("PartialNavbar", "Default")
    <br />
    <div class="reservation-form">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div id="map">
                       
                    </div>
                </div>
                <div class="col-lg-12">
                    <form id="reservation-form" name="gs" method="submit" role="search" action="#">
                        <div class="row">
                            <div class="col-lg-12">
                                <h4>Please <em>Sign up</em>  This <em>Form</em></h4>
                            </div>
                            <form method="post">
                                <div class="container">
                                    <br /><br />
                                    
                                    <label>Adiniz</label>
                                    @Html.TextBoxFor(x => x.Name, new { @class = "form-control" })
                                    <br />
                                    <label>Soyadiniz</label>
                                    @Html.TextBoxFor(x => x.Surname, new { @class = "form-control" })
                                    <br />
                                    <label>E-Mail Adresiniz</label>
                                    @Html.TextBoxFor(x => x.Email, new { @class = "form-control" })
                                    <br />
                                    <label>Kullanici Adiniz</label>
                                    @Html.TextBoxFor(x => x.Username, new { @class = "form-control" })
                                    <br />
                                    <label>Sifreniz</label>
                                    @Html.PasswordFor(x => x.Pasword, new { @class = "form-control" })
                                    <button class="btn btn-primary">Save User !</button>
                                </div>
                            </form>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>

    @Html.Action("PartialFooter", "Default")

    @Html.Action("PartialScript", "Default")
</body>
</html>
